
@{
    ViewBag.Title = "AMCOMReports";
    Layout = "~/Views/Home/HomeLayout.cshtml";
}


<div id="page-wrapper">

    <div class="container-fluid">

        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">
                   <small>@ViewBag.ReportTitle</small>
                </h1>
                <ol class="breadcrumb">
                    <li class="active">
                        <i class="fa fa-dashboard"></i> Dashboard
                    </li>
                </ol>
            </div>
        </div>
        <!-- /.row -->
        <div class="=row">
        <div class="form-group">
            <label>StartDate</label>
            <input id="pick" class="form-control" style="width:10%">
            <p class="help-block">Please select report start date</p>
        </div>
       </div>

    </div>
    <!-- /.container-fluid -->

</div>

